<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:divider="?android:dividerHorizontal"
        android:showDividers="middle"
        android:orientation="horizontal"
        android:stretchColumns="*"
        tools:ignore="MissingConstraints">
        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:divider="?android:dividerHorizontal"
            android:showDividers="middle">
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/idClick"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginVertical="5dp"
                android:layout_centerHorizontal="true"
                card_view:cardBackgroundColor="#E6E6E6"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <TextView
                    android:id="@+id/txtLogProvisioningId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fontFamily="sans-serif"
                    android:textAlignment="center"
                    android:textSize="16dp"
                    android:text="No"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/timestampClick"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginVertical="5dp"
                android:layout_centerHorizontal="true"
                card_view:cardBackgroundColor="#E6E6E6"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <TextView
                    android:id="@+id/txtLogProvisioningTimestamp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:fontFamily="sans-serif"
                    android:textSize="16dp"
                    android:text="Timestamp"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/custnameClick"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginVertical="5dp"
                android:layout_centerHorizontal="true"
                card_view:cardBackgroundColor="#E6E6E6"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <TextView
                    android:id="@+id/txtLogProvisioningCustName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:fontFamily="sans-serif"
                    android:textSize="16dp"
                    android:text="Cust_Name"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/custaddrClick"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginVertical="5dp"
                android:layout_centerHorizontal="true"
                card_view:cardBackgroundColor="#E6E6E6"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <TextView
                    android:id="@+id/txtLogProvisioningCustAddr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:fontFamily="sans-serif"
                    android:textSize="16dp"
                    android:text="Cust_Addr"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:card_view="http://schemas.android.com/apk/res-auto"
                android:id="@+id/statusClick"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_marginVertical="5dp"
                android:layout_centerHorizontal="true"
                card_view:cardBackgroundColor="#E6E6E6"
                android:foreground="?android:attr/selectableItemBackground"
                android:clickable="true"
                card_view:cardCornerRadius="2dp"
                card_view:cardElevation="10dp">
                <TextView
                    android:id="@+id/txtLogProvisioningStatus"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:fontFamily="sans-serif"
                    android:textSize="16dp"
                    android:text="Status"/>
            </androidx.cardview.widget.CardView>
        </TableRow>
    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>